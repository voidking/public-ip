{
  "manifest_version": 2,

  "name": "Public IP",
  "description": "This extension shows your public IP",
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "64": "icon_64.png",
    "128": "icon_128.png"
  },
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon_19.png",
    "default_title": "查看公网IP",
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://ip.chinaz.com/getip.aspx"
  ]
}
